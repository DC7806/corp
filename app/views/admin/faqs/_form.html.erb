<%= form_for admin_faq do |f| %>
<div class="form-group">
  <%= f.label :lang %>
  <%= f.text_field :lang %>
</div>
<div class="form-group">
  <%= f.label :question %>
  <%= f.text_field :question %>
</div>
<div class="form-group">
  <%= f.label :answer %>
  <%= f.text_area :answer %>
</div>
<div class="form-group">
  <%= f.submit "Submit" %>
</div>

<% end %>